<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="900.0" stylesheets="@endGame.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EndGameController">
   <children>
      <VBox prefHeight="600.0" prefWidth="900.0" spacing="200.0">
         <children>
            <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" prefHeight="100.0" prefWidth="900.0" text="GAME OVER : " textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font size="22.0" />
               </font></Label>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="200.0">
               <children>
                  <Button fx:id="buttonQuit" mnemonicParsing="false" prefHeight="80.0" prefWidth="150.0" text="Quitter" textFill="WHITE" />
                  <Button fx:id="buttonPlayAgain" mnemonicParsing="false" prefHeight="80.0" prefWidth="150.0" text="Rejouer" textFill="WHITE" />
               </children>
               <padding>
                  <Insets left="200.0" top="10.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="50.0" top="150.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
